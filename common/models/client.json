{
  "name": "client",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true
  },
  "properties": {
    "mobile": {
      "type": "string"
    },
    "formID": {
      "type": "string"
    },
    "clientNumber": {
      "type": "number",
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "forms": {
      "type": "hasMany",
      "model": "forms",
      "foreignKey": "clientId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "manager",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
